    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "react-redux": "^8.1.3",
    "redux-logger": "^3.0.6",
    "sass": "^1.69.5",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
